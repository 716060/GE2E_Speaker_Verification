    "TI_SV_train": {
      "N": 4,
      "M": 10,
      "num_workers": 0,
      "lr": 0.01,
      "epochs": 950,
      "log_intervals": 30,
      "log_file": "./log/tisv/log.log",
      "checkpoint_interval": 120,
      "checkpoint_dir": "./checkpoint/tisv/",
      "restore": false
    }
  },
  "test": {
    "TD_SV_test": {
      "N": 4,
      "M": 5,
      "num_workers": 8,
      "epochs": 10
